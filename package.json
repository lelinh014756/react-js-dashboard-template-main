    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@/public/(.*)$": "<rootDir>/public/$1"
    },
    "clearMocks": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,ts,tsx}",
      "!./src/**/_*.{js,jsx,ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 30,
        "lines": 30,
        "statements": 30
      }
    },
    "testEnvironment": "jest-environment-jsdom"
  }
